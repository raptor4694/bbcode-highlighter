                },
                {
                    "name": "markup.usertag.bb",
                    "match": "@@[-a-z0-9]{1,30}\\b"
                },
                {
                    "name": "markup.usertag.bb",
                    "match": "@[-a-z0-9]{1,30}\\b"
                }
            ]
        },
